{# Jinja2 basic template for EOT pages
## Extend with other template files by using {% extends "base.html.jinja" %}
## -----------------------------------------------------------------------------
## Constituent blocks:
## title     : should contain string to precede " - Excavating Occaneechi Town"
## styles    : should contain links to all stylesheets for the page
## header    : should contain the page's navbar
## page_body : should contain the page's specific content, including the
##             sidebar, any images or text, etc.
## -----------------------------------------------------------------------------
## Required parameters:
##  none
## -----------------------------------------------------------------------------
#}
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <title>{% block title %}{% endblock %} - Excavating Occaneechi Town</title>
    {% block styles %}
    {% include "shared_elements/common_css_and_fonts.html.jinja" %}
    {% endblock %}

    <!-- Favicons generated by realfavicongenerator.net -->
    <link rel="apple-touch-icon" sizes="180x180" href="../../assets/img/favicon/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="../../assets/img/favicon/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="../../assets/img/favicon/favicon-16x16.png">
    <link rel="manifest" href="../../assets/img/favicon/site.webmanifest">
    <link rel="mask-icon" href="../../assets/img/favicon/safari-pinned-tab.svg" color="#5bbad5">
    <link rel="shortcut icon" href="../../assets/img/favicon/favicon.ico">
    <meta name="msapplication-TileColor" content="#da532c">
    <meta name="msapplication-config" content="../../assets/img/favicon/browserconfig.xml">
    <meta name="theme-color" content="#ffffff">
  </head>

  <body data-spy="scroll" data-target="#toc">
    {% block header %}
    <header>
      <div class="card" id="h-card">
        <div class="card-header d-flex">
          <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#eot-sidebar" aria-expanded="false" aria-controls="eot-sidebar" aria-expanded="false" aria-label="Toggle navigation" id="sidebarButton">
            <span class="navbar-toggler-icon"></span>
          </button>
          <a class="mr-auto mt-1" href="../index.html">
            <img id="bar-img-wide-base" src="../../assets/img/title_top_long.png">
            <img id="bar-img-narrow-base" src="../../assets/img/title_top.png">
          </a>
          <a class="mt-1" href="https://uncpress.org">
            <img id="press-logo" src="../../assets/img/press_logo.png">
          </a>
        </div>
      </div>
    </header>
    {% endblock %}
    {% block page_body %}{% endblock %}

    <!-- Bootstrap core JavaScript -->
    <script src="../../assets/js/jquery.min.js"></script>
    <script src="../../assets/js/bootstrap.min.js"></script>
    <script src="../../assets/js/bootstrap-toc.min.js"></script>
    {% block site_interactivity %}{% endblock %}
  </body>
</html>
