{# Jinja2 basic template for EOT pages
## Extend with other template files by using {% extends "base.html.jinja" %}
## -----------------------------------------------------------------------------
## Constituent blocks:
## title     : should contain string to precede " - Excavating Occaneechi Town"
## styles    : should contain links to all stylesheets for the page
## header    : should contain the page's navbar
## page_body : should contain the page's specific content, including the
##             sidebar, any images or text, etc.
## -----------------------------------------------------------------------------
## Required parameters:
##  none
## -----------------------------------------------------------------------------
#}
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <title>{% block title %}{% endblock %} - Excavating Occaneechi Town</title>
    {% block styles %}
    <link href="../../assets/css/bootstrap.min.css" rel="stylesheet">
    {% endblock %}
  </head>

  <body data-spy="scroll" data-target="#toc">
    {% block header %}
    <header class="container-fluid sticky-top bg-dark">
      <div class="navbar navbar-dark flex-column flex-md-row align-items-start bg-dark eot-navbar">
        <a class="navbar-brand" href="../index.html">Excavating Occaneechi Town</a>
        <button class="btn btn-secondary" type="button" data-toggle="collapse" data-target="#eot-sidebar" aria-expanded="false" aria-controls="eot-sidebar">
          Toggle Sidebar
        </button>
      </div>
    </header>
    {% endblock %}
    {% block page_body %}{% endblock %}

    <!-- Bootstrap core JavaScript -->
    <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ho+j7jyWK8fNQe+A12Hb8AhRq26LrZ/JpcUGGOn+Y7RsweNrtN/tE3MoK7ZeZDyx" crossorigin="anonymous"></script>
    <script src="https://cdn.rawgit.com/afeld/bootstrap-toc/v1.0.1/dist/bootstrap-toc.min.js"></script>
    {% block site_interactivity %}{% endblock %}
  </body>
</html>
